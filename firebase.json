{
  "hosting": [
    {
      "site": "thekosmix5",
      "public": "thekosmix.com",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        ".git/**",
        ".gitignore",
        ".project"
      ],
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|webp|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "**/*.@(js|css)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        }
