{
  "hosting": [
    {
      "site": "thekosmix5",
      "public": "thekosmix.com",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        ".git/**",
        ".gitignore",
        "QWEN.md",
        "GEMINI.md",
        ".project"
      ],
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|webp|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "**/*.@(js|css|html)",
          "headers": [
            {
              "key": "Cache-Control",
